{
      "Args": {
        "Description": "",
        "Name": "",
        "Settable": null,
        "Value": null
      },
      "Description": "REX-Ray for Digital Ocean Block Storage",
      "Documentation": "https://github.com/codedellemc/rexray/.docker/plugins/dobs",
      "Entrypoint": [
        "/rexray.sh", "rexray", "start", "-f", "--nopid"
      ],
      "Env": [
        {
          "Description": "",
          "Name": "REXRAY_FSTYPE",
          "Settable": [
            "value"
          ],
          "Value": "ext4"
        },
        {
          "Description": "",
          "Name": "REXRAY_LOGLEVEL",
          "Settable": [
            "value"
          ],
          "Value": "warn"
        },
        {
          "Description": "",
          "Name": "REXRAY_PREEMPT",
          "Settable": [
            "value"
          ],
          "Value": "false"
        },
        {
          "Description": "",
          "Name": "DOBS_CONVERTUNDERSCORES",
          "Settable": [
            "value"
          ],
          "Value": "false"
        },
        {
          "Description": "",
          "Name": "DOBS_REGION",
          "Settable": [
            "value"
          ],
          "Value": ""
        },
        {
          "Description": "",
          "Name": "DOBS_STATUSINITIALDELAY",
          "Settable": [
            "value"
          ],
          "Value": ""
        },
        {
          "Description": "",
          "Name": "DOBS_STATUSMAXATTEMPTS",
          "Settable": [
            "value"
          ],
          "Value": ""
        },
        {
          "Description": "",
          "Name": "DOBS_STATUSTIMEOUT",
          "Settable": [
            "value"
          ],
          "Value": ""
        },
        {
          "Description": "",
          "Name": "DOBS_TOKEN",
          "Settable": [
            "value"
          ],
          "Value": ""
        }
      ],
      "Interface": {
        "Socket": "rexray.sock",
        "Types": [
          "docker.volumedriver/1.0"
        ]
      },
      "Linux": {
        "AllowAllDevices": true,
        "Capabilities": ["CAP_SYS_ADMIN"],
        "Devices": null
      },
      "Mounts": [
        {
          "Source": "/dev",
          "Destination": "/dev",
          "Type": "bind",
          "Options": ["rbind"]
        }
      ],
      "Network": {
        "Type": "host"
      },
      "PropagatedMount": "/var/lib/rexray/volumes",
      "User": {},
      "WorkDir": ""
}
